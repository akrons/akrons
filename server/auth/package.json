{
  "name": "@akrons/auth-service",
  "license": "Apache-2.0",
  "version": "1.0.0",
  "files": [
    "package*.json",
    "README.md",
    "dist/**"
  ],
  "description": "",
  "main": "dist/index",
  "types": "dist/index",
  "author": "",
  "scripts": {
    "start": "node dist/index.js",
    "start-ts": "node src/index.ts",
    "build": "tsc -p .",
    "build:prod": "npm run build",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "typedoc": "node node_modules/typedoc/bin/typedoc --out ../docs/api/utils --module commonjs --mode file --target es6 --readme none --name \"hvstr-utils API Documentation\" --tsconfig ./tsconfig.json --ignoreCompilerErrors ./src/",
    "publish:npm": "npm publish",
    "docker": "node ../../docker-build.js --service auth",
    "docker:latest": "node ../../docker-build.js --service auth --latest"
  },
  "dependencies": {
    "@akrons/auth-lib": "1.0.0",
    "@akrons/common-auth": "1.0.0",
    "@akrons/configuration": "1.0.0",
    "@akrons/service-utils": "1.0.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.4",
    "redis": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@akrons/types": "1.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mongodb": "^3.6.9",
    "@types/node": "^14.14.34",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
