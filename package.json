{
  "name": "@akrons/akrons",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "publish": "lerna exec --no-private -- npm publish --access public",
    "publish:since": "lerna exec --no-private --since $(git rev-parse HEAD^) -- npm publish --access public",
    "version": "lerna version --conventional-commits --yes",
    "build": "lerna run build",
    "build:prod": "lerna run build:prod",
    "docker": "lerna run docker",
    "docker:latest": "lerna run docker:latest --since $(git rev-parse HEAD^)",
    "clean": " lerna clean --yes",
    "start": "lerna run start"
  },
  "devDependencies": {
    "lerna": "^3.22.1"
  }
}
